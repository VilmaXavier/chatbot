{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONUNBUFFERED": "true"
  },
  "installCommand": "pip install --no-cache-dir -r requirements.txt",
  "buildCommand": "",
  "pythonVersion": "3.12"
}
